{"ast":null,"code":"import AddModal from \"@/components/AddModal\";\nimport { mapGetters } from 'vuex';\nexport default {\n  name: \"ClientList\",\n  components: {\n    AddModal\n  },\n  data() {\n    return {\n      modal1: null,\n      inputDisabled: false,\n      isOpened: false\n    };\n  },\n  watch: {\n    checkedProducts() {\n      if (this.checkedProducts.length > 1) {\n        this.modal1 = null;\n      } else {\n        if (this.checkedProducts.length === 1) {\n          this.modal1 = \"modal\";\n        } else {\n          this.inputDisabled = false;\n          this.modal1 = null;\n        }\n        this.inputDisabled = false;\n      }\n    }\n  },\n  methods: {\n    addElement() {\n      this.modal1 = \"modal\";\n      this.isOpened = true;\n    },\n    openModalEdit() {\n      if (this.checkedProducts.length !== 1) {\n        this.modal1 = null;\n        alert(\"Выберете одного пользователя\");\n      } else {\n        this.isOpened = true;\n        this.choosenProduct = this.items[this.checkedProducts[0]];\n      }\n    },\n    changeProduct(newProductsData) {\n      this.$emit('parent_change_product', newProductsData);\n    }\n  },\n  computed: {\n    ...mapGetters([\"allClients\"]),\n    isModalOpen() {\n      return this.$store.state.modal.isModalOpen; // Первое вычисляемое свойство\n    }\n  },\n\n  async mounted() {\n    this.$store.dispatch('fetchClients');\n  }\n};","map":{"version":3,"names":["AddModal","mapGetters","name","components","data","modal1","inputDisabled","isOpened","watch","checkedProducts","length","methods","addElement","openModalEdit","alert","choosenProduct","items","changeProduct","newProductsData","$emit","computed","isModalOpen","$store","state","modal","mounted","dispatch"],"sources":["C:\\Users\\aser\\PhpstormProjects\\exercise2\\src\\views\\ClientList.vue"],"sourcesContent":["<template>\r\n  <table class=\"table\">\r\n    <thead>\r\n    <tr>\r\n      <th scope=\"col\"></th>\r\n      <th scope=\"col\">Client Id</th>\r\n      <th scope=\"col\">Lastname</th>\r\n      <th scope=\"col\">Firstname</th>\r\n      <th scope=\"col\"><button type=\"button\" class=\"btn btn-success\" :data-bs-toggle=\"modal1\" data-bs-target=\"#addModal\"\r\n                                                      @click=\"addElement\">Добавить</button></th>\r\n      <th scope=\"col\"></th>\r\n    </tr>\r\n    </thead>\r\n    <tbody>\r\n    <tr v-for=\"item in allClients\" :key=\"item.id\">\r\n      <input class=\"form-check-input\" type=\"checkbox\" :value=\"index\" v-model=\"checkedProducts\"\r\n             :disabled=\"inputDisabled\" :id=\"item.id\">\r\n      <th scope=\"col\"></th>\r\n      <td>{{ item.id }}</td>\r\n      <td>{{ item.last_name_doc }}</td>\r\n      <td>{{ item.first_name_doc }}</td>\r\n      <td class=\"btntd\"><button type=\"button\" class=\"btn btn-warning\" :data-bs-toggle=\"modal1\" data-bs-target=\"#exampleModal\"\r\n                  @click=\"openModalEdit\">Редактировать</button></td>\r\n      <td class=\"btntd\"><button type=\"button\" class=\"btn btn-danger\" @click=\"changedelete\">Удалить</button></td>\r\n      <!-- Вывод других данных -->\r\n    </tr>\r\n    </tbody>\r\n  </table>\r\n  <AddModal/>\r\n</template>\r\n\r\n<script>\r\nimport AddModal from \"@/components/AddModal\";\r\nimport {mapGetters} from 'vuex';\r\n\r\n\r\nexport default {\r\n  name: \"ClientList\",\r\n  components: {\r\n    AddModal\r\n  },\r\n  data() {\r\n    return {\r\n      modal1: null,\r\n      inputDisabled: false,\r\n      isOpened: false,\r\n    }\r\n  },\r\n  watch: {\r\n    checkedProducts(){\r\n      if (this.checkedProducts.length > 1){\r\n        this.modal1 = null\r\n      }else{\r\n        if(this.checkedProducts.length === 1){\r\n          this.modal1 = \"modal\"\r\n        }else {\r\n          this.inputDisabled =false\r\n          this.modal1 = null\r\n        }\r\n        this.inputDisabled =false\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n\r\n    addElement(){\r\n      this.modal1 = \"modal\"\r\n      this.isOpened = true\r\n    },\r\n    openModalEdit() {\r\n      if (this.checkedProducts.length !== 1){\r\n        this.modal1 = null\r\n        alert(\"Выберете одного пользователя\")\r\n      }\r\n      else{\r\n        this.isOpened = true\r\n        this.choosenProduct = this.items[this.checkedProducts[0]]\r\n      }\r\n    },\r\n    changeProduct(newProductsData) {\r\n      this.$emit('parent_change_product', newProductsData)\r\n    },\r\n  },\r\n  computed:{\r\n      ...mapGetters([\"allClients\"]),\r\n    isModalOpen() {\r\n      return this.$store.state.modal.isModalOpen; // Первое вычисляемое свойство\r\n    },\r\n  },\r\n\r\n  async mounted() {\r\n    this.$store.dispatch('fetchClients')\r\n  }\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.btntd{\r\n  width: 150px;\r\n}\r\n</style>"],"mappings":"AAgCA,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,SAAQC,UAAU,QAAO,MAAM;AAG/B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAK,EAAE;IACLC,eAAeA,CAAA,EAAE;MACf,IAAI,IAAI,CAACA,eAAe,CAACC,MAAK,GAAI,CAAC,EAAC;QAClC,IAAI,CAACL,MAAK,GAAI,IAAG;MACnB,CAAC,MAAI;QACH,IAAG,IAAI,CAACI,eAAe,CAACC,MAAK,KAAM,CAAC,EAAC;UACnC,IAAI,CAACL,MAAK,GAAI,OAAM;QACtB,CAAC,MAAK;UACJ,IAAI,CAACC,aAAY,GAAG,KAAI;UACxB,IAAI,CAACD,MAAK,GAAI,IAAG;QACnB;QACA,IAAI,CAACC,aAAY,GAAG,KAAI;MAC1B;IACF;EACF,CAAC;EACDK,OAAO,EAAE;IAEPC,UAAUA,CAAA,EAAE;MACV,IAAI,CAACP,MAAK,GAAI,OAAM;MACpB,IAAI,CAACE,QAAO,GAAI,IAAG;IACrB,CAAC;IACDM,aAAaA,CAAA,EAAG;MACd,IAAI,IAAI,CAACJ,eAAe,CAACC,MAAK,KAAM,CAAC,EAAC;QACpC,IAAI,CAACL,MAAK,GAAI,IAAG;QACjBS,KAAK,CAAC,8BAA8B;MACtC,OACI;QACF,IAAI,CAACP,QAAO,GAAI,IAAG;QACnB,IAAI,CAACQ,cAAa,GAAI,IAAI,CAACC,KAAK,CAAC,IAAI,CAACP,eAAe,CAAC,CAAC,CAAC;MAC1D;IACF,CAAC;IACDQ,aAAaA,CAACC,eAAe,EAAE;MAC7B,IAAI,CAACC,KAAK,CAAC,uBAAuB,EAAED,eAAe;IACrD;EACF,CAAC;EACDE,QAAQ,EAAC;IACL,GAAGnB,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAC/BoB,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,C