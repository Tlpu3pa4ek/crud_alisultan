uct = this.items[this.checkedProducts[0]]\r\n      }\r\n    },\r\n    changeProduct(newProductsData) {\r\n      this.$emit('parent_change_product', newProductsData)\r\n    },\r\n  },\r\n  computed:{\r\n      ...mapGetters([\"allClients\"]),\r\n    isModalOpen() {\r\n      return this.$store.state.modal.isModalOpen; // Первое вычисляемое свойство\r\n    },\r\n  },\r\n\r\n  async mounted() {\r\n    this.$store.dispatch('fetchClients')\r\n  }\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.btntd{\r\n  width: 150px;\r\n}\r\n</style>"],"mappings":"AAgCA,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,SAAQC,UAAU,QAAO,MAAM;AAG/B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,WAAW,CAAC,EAAE;IACrC,CAAC;;IACDC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACF,MAAM,CAACC,QAAQ,CAAC,YAAY,CAAC,EAAE;IACtC,CAAC;;IACDE,UAAUA,CAAA,EAAE;MACV,IAAI,CAACR,MAAK,GAAI,OAAM;MACpB,IAAI,CAACE,QAAO,GAAI,IAAG;IACrB,CAAC;IACDO,aAAaA,CAAA,EAAG;MACd,IAAI,IAAI,CAACC,eAAe,CAACC,MAAK,KAAM,CAAC,EAAC;QACpC,IAAI,CAACX,MAAK,GAAI,IAAG;QACjBY,KAAK,CAAC,8BAA8B;MACtC,OACI;QACF,IAAI,CAACV,QAAO,GAAI,IAAG;QACnB,IAAI,CAACW,cAAa,GAAI,IAAI,CAACC,KAAK,CAAC,IAAI,CAACJ,eAAe,CAAC,CAAC,CAAC;MAC1D;IACF,CAAC;IACDK,aAAaA,CAACC,eAAe,EAAE;MAC7B,IAAI,CAACC,KAAK,CAAC,uBAAuB,EAAED,eAAe;IACrD;EACF,CAAC;EACDE,QAAQ,EAAC;IACL,GAAGtB,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAC/BuB,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACd,MAAM,CAACe,KAAK,CAACC,KAAK,CAACF,WAAW,EAAE;IAC9C;EACF,CAAC;;EAED,MAAMG,OAAOA,CAAA,EAAG;IACd,IAAI,CAACjB,MAAM,CAACC,QAAQ,CAAC,cAAc;EACrC;AAEF"},"metadata":{},"sourceType":"module","externalDependencies":[]}