odal2 = \"modal\"\r\n      this.isOpened = true\r\n    },\r\n    openModalEdit() {\r\n      if (this.checkedClient.length !== 1){\r\n        this.modal2 = null\r\n        alert(\"Выберете одного пользователя\")\r\n        \r\n      }\r\n      else{\r\n        this.isOpened = true\r\n        this.choosenClient = this.items[this.checkedClient[0]]\r\n      }\r\n    },\r\n  },\r\n  computed:{\r\n      ...mapGetters([\"allClients\"]),\r\n    isModalOpen() {\r\n      return this.$store.state.modal.isModalOpen; // Первое вычисляемое свойство\r\n    },\r\n  },\r\n\r\n  async mounted() {\r\n    this.$store.dispatch('fetchClients')\r\n  }\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.btntd{\r\n  width: 150px;\r\n}\r\n</style>"],"mappings":"AAgCA,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,SAAQC,UAAU,QAAO,MAAM;AAG/B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,MAAM,EAAE,IAAI;MACZC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE,KAAK;MACfC,aAAa,EAAE,CAAC,CAAC;MACjBC,aAAa,EAAC;IAChB;EACF,CAAC;EACDC,KAAK,EAAE;IACLD,aAAaA,CAAA,EAAE;MACb,IAAI,IAAI,CAACA,aAAa,CAACE,MAAK,GAAI,CAAC,EAAC;QAChC,IAAI,CAACN,MAAK,GAAI,IAAG;MACnB,CAAC,MAAI;QACH,IAAG,IAAI,CAACI,aAAa,CAACE,MAAK,KAAM,CAAC,EAAC;UACjC,IAAI,CAACN,MAAK,GAAI,OAAM;QACtB,CAAC,MAAK;UACJ,IAAI,CAACC,aAAY,GAAG,KAAI;UACxB,IAAI,CAACD,MAAK,GAAI,IAAG;QACnB;QACA,IAAI,CAACC,aAAY,GAAG,KAAI;MAC1B;IACF;EACF,CAAC;EACDM,OAAO,EAAE;IAEPC,UAAUA,CAAA,EAAE;MACV,IAAI,CAACR,MAAK,GAAI,OAAM;MACpB,IAAI,CAACE,QAAO,GAAI,IAAG;IACrB,CAAC;IACDO,aAAaA,CAAA,EAAG;MACd,IAAI,IAAI,CAACL,aAAa,CAACE,MAAK,KAAM,CAAC,EAAC;QAClC,IAAI,CAACN,MAAK,GAAI,IAAG;QACjBU,KAAK,CAAC,8BAA8B;MAEtC,OACI;QACF,IAAI,CAACR,QAAO,GAAI,IAAG;QACnB,IAAI,CAACC,aAAY,GAAI,IAAI,CAACQ,KAAK,CAAC,IAAI,CAACP,aAAa,CAAC,CAAC,CAAC;MACvD;IACF;EACF,CAAC;EACDQ,QAAQ,EAAC;IACL,GAAGjB,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAC/BkB,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,KAAK,CAACH,WAAW,EAAE;IAC9C;EACF,CAAC;;EAED,MAAMI,OAAOA,CAAA,EAAG;IACd,IAAI,CAACH,