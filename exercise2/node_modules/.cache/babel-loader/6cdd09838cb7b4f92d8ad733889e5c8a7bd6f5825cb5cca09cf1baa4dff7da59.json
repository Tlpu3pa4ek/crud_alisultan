{"ast":null,"code":"import ProductModal from \"@/components/products/ProductModal\";\nimport AddModal from \"@/components/products/AddModal\";\nexport default {\n  name: \"InfoProducts\",\n  components: {\n    ProductModal,\n    AddModal\n  },\n  props: {\n    items: Array\n  },\n  data() {\n    return {\n      modal1: null,\n      modal2: null,\n      checkedProducts: [],\n      inputDisabled: false,\n      isOpened: false,\n      choosenProduct: {}\n    };\n  },\n  watch: {\n    checkedProducts() {\n      if (this.checkedProducts.length > 1) {\n        this.modal1 = null;\n      } else {\n        if (this.checkedProducts.length === 1) {\n          this.modal1 = \"modal\";\n        } else {\n          this.inputDisabled = false;\n          this.modal1 = null;\n        }\n        this.inputDisabled = false;\n      }\n    }\n  },\n  methods: {\n    addElement() {\n      this.modal2 = \"modal\";\n      this.isOpened = true;\n    },\n    changedelete() {\n      {\n        this.$emit('change_delete', {\n          'last_name_doc': this.last_name_doc,\n          'first_name_doc': this.first_name_doc,\n          'id': this.choosenProduct.id\n        });\n      }\n    },\n    openModalEdit() {\n      if (this.checkedProducts.length !== 1) {\n        this.modal1 = null;\n        alert(\"Выберете одного пользователя\");\n      } else {\n        this.isOpened = true;\n        this.choosenProduct = this.items[this.checkedProducts[0]];\n      }\n    },\n    changeProduct(newProductsData) {\n      this.$emit('parent_change_product', newProductsData);\n    }\n  }\n\n  /////////////////////////////////\n};","map":{"version":3,"names":["ProductModal","AddModal","name","components","props","items","Array","data","modal1","modal2","checkedProducts","inputDisabled","isOpened","choosenProduct","watch","length","methods","addElement","changedelete","$emit","last_name_doc","first_name_doc","id","openModalEdit","alert","changeProduct","newProductsData"],"sources":["C:\\Users\\aser\\PhpstormProjects\\exercise2\\src\\components\\products\\InfoProducts.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div>\r\n      <button type=\"button\" class=\"btn btn-success\" :data-bs-toggle=\"modal2\" data-bs-target=\"#addModal\"\r\n              @click=\"addElement\">Добавить</button>\r\n      <button type=\"button\" class=\"btn btn-warning\" :data-bs-toggle=\"modal1\" data-bs-target=\"#exampleModal\"\r\n              @click=\"openModalEdit\">Редактировать</button>\r\n      <button type=\"button\" class=\"btn btn-danger\" @click=\"changedelete\">Удалить</button>\r\n\r\n    </div>\r\n    <table class=\"table\">\r\n      <thead>\r\n      <tr>\r\n        <th scope=\"col\"></th>\r\n        <th scope=\"col\">Id</th>\r\n        <th scope=\"col\">Lastname</th>\r\n        <th scope=\"col\">Firstname</th>\r\n        <th scope=\"col\">Rating</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"(item, index) in items\" :key=\"item.id\">\r\n        <input class=\"form-check-input\" type=\"checkbox\" :value=\"index\" v-model=\"checkedProducts\"\r\n               :disabled=\"inputDisabled\" :id=\"item.id\">\r\n        <th scope=\"row\"><label class=\"form-check-label\" :for=\"item.id\">\r\n          {{ item.id }}\r\n        </label></th>\r\n        <td><label class=\"form-check-label\" :for=\"item.id\">\r\n          {{ item.last_name_doc}}\r\n        </label></td>\r\n        <td><label class=\"form-check-label\" :for=\"item.id\">\r\n          {{ item.first_name_doc}}\r\n        </label></td>\r\n        <td><label class=\"form-check-label\" :for=\"item.id\">\r\n          {{ item.rating}}\r\n        </label></td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n\r\n  <ProductModal :choosenProduct=\"choosenProduct\" @change_product=\"changeProduct\"/>\r\n  <AddModal/>\r\n</template>\r\n\r\n<script>\r\n\r\nimport ProductModal from \"@/components/products/ProductModal\";\r\nimport AddModal from \"@/components/products/AddModal\";\r\n\r\nexport default {\r\n  name: \"InfoProducts\",\r\n  components:{\r\n    ProductModal,\r\n    AddModal\r\n  },\r\n  props: {\r\n    items: Array,\r\n  },\r\n  data() {\r\n    return {\r\n      modal1: null,\r\n      modal2: null,\r\n      checkedProducts: [],\r\n      inputDisabled: false,\r\n      isOpened: false,\r\n      choosenProduct: {}\r\n    }\r\n  },\r\n  watch: {\r\n    checkedProducts(){\r\n      if (this.checkedProducts.length > 1){\r\n        this.modal1 = null\r\n      }else{\r\n        if(this.checkedProducts.length === 1){\r\n          this.modal1 = \"modal\"\r\n        }else {\r\n          this.inputDisabled =false\r\n          this.modal1 = null\r\n        }\r\n        this.inputDisabled =false\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    addElement(){\r\n      this.modal2 = \"modal\"\r\n      this.isOpened = true\r\n    },\r\n\r\n    changedelete() {{\r\n      this.$emit('change_delete', {'last_name_doc' : this.last_name_doc, 'first_name_doc' : this.first_name_doc, 'id': this.choosenProduct.id})\r\n    }\r\n    },\r\n    openModalEdit() {\r\n      if (this.checkedProducts.length !== 1){\r\n        this.modal1 = null\r\n        alert(\"Выберете одного пользователя\")\r\n      }\r\n      else{\r\n        this.isOpened = true\r\n        this.choosenProduct = this.items[this.checkedProducts[0]]\r\n      }\r\n    },\r\n    changeProduct(newProductsData) {\r\n      this.$emit('parent_change_product', newProductsData)\r\n    },\r\n  }\r\n\r\n/////////////////////////////////\r\n\r\n\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n#users, #users_action {\r\n  margin-top: 2%;\r\n}\r\n\r\n#users_action button {\r\n  margin-left: 2%;\r\n}\r\n#users_card {\r\n  margin-top: 2%;\r\n}\r\n.cards {\r\n  margin-bottom: 2%;\r\n}\r\n\r\n.btn {\r\n  margin: 1%;\r\n}\r\n</style>"],"mappings":"AA+CA,OAAOA,YAAW,MAAO,oCAAoC;AAC7D,OAAOC,QAAO,MAAO,gCAAgC;AAErD,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAC;IACTH,YAAY;IACZC;EACF,CAAC;EACDG,KAAK,EAAE;