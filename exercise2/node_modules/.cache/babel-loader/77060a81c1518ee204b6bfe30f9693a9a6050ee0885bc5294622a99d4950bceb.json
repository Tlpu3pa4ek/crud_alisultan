modal1: null,\r\n      modal2: null,\r\n      checkedUsers: [],\r\n      inputDisabled: false,\r\n      isOpened: false,\r\n      choosenProduct: {}\r\n    }\r\n  },\r\n  watch: {\r\n    checkedUsers(){\r\n      if (this.checkedUsers.length > 1){\r\n        this.modal1 = null\r\n      }else{\r\n        if(this.checkedUsers.length === 1){\r\n          this.modal1 = \"modal\"\r\n        }else {\r\n          this.inputDisabled =false\r\n          this.modal1 = null\r\n        }\r\n        this.inputDisabled =false\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    addElement(){\r\n      this.modal2 = \"modal\"\r\n      this.isOpened = true\r\n    },\r\n\r\n    removeElement(newdata) {{\r\n      this.$emit('change_delete', newdata)\r\n    }\r\n    },\r\n    openModalEdit() {\r\n      if (this.checkedUsers.length !== 1){\r\n        this.modal1 = null\r\n        alert(\"Выберете одного пользователя\")\r\n      }\r\n      else{\r\n        this.isOpened = true\r\n        this.choosenProduct = this.items[this.checkedUsers[0]]\r\n      }\r\n    },\r\n    changeProduct(newProductsData) {\r\n      this.$emit('parent_change_product', newProductsData)\r\n    },\r\n  }\r\n\r\n/////////////////////////////////\r\n\r\n\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n#users, #users_action {\r\n  margin-top: 2%;\r\n}\r\n\r\n#users_action button {\r\n  margin-left: 2%;\r\n}\r\n#users_card {\r\n  margin-top: 2%;\r\n}\r\n.cards {\r\n  margin-bottom: 2%;\r\n}\r\n\r\n.btn {\r\n  margin: 1%;\r\n}\r\n</style>"],"mappings":"AA+CA,OAAOA,YAAW,MAAO,oCAAoC;AAC7D,OAAOC,QAAO,MAAO,gCAAgC;AAErD,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAC;IACTH,YAAY;IACZC;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,KAAK,EAAEC;EACT,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,MAAM,EAAE,IAAI;MACZC,YAAY,EAAE,EAAE;MAChBC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE,KAAK;MACfC,cAAc,EAAE,CAAC;IACnB;EACF,CAAC;EACDC,KAAK,EAAE;IACLJ,YAAYA,CAAA,EAAE;MACZ,IAAI,IAAI,CAACA,YAAY,CAACK,MAAK,GAAI,CAAC,EAAC;QAC/B,IAAI,CAACP,MAAK,GAAI,