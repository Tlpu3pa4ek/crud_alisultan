{"ast":null,"code":"import ProductModal from \"@/components/products/ProductModal\";\nexport default {\n  name: \"InfoProducts\",\n  components: {\n    ProductModal\n  },\n  props: {\n    items: Array\n  },\n  data() {\n    return {\n      modal1: null,\n      modal2: null,\n      checkedUsers: [],\n      inputDisabled: false,\n      isOpened: false,\n      choosenUser: {}\n    };\n  },\n  watch: {\n    checkedUsers() {\n      if (this.checkedUsers.length > 1) {\n        this.modal1 = null;\n      } else {\n        if (this.checkedUsers.length === 1) {\n          this.modal1 = \"modal\";\n        } else {\n          this.inputDisabled = false;\n          this.modal1 = null;\n        }\n        this.inputDisabled = false;\n      }\n    }\n  },\n  methods: {\n    addElement() {\n      this.modal2 = \"modal\";\n    },\n    removeElement() {\n      this.checkedUsers;\n    },\n    openModalEdit() {\n      if (this.checkedUsers.length !== 1) {\n        this.modal1 = null;\n        alert(\"Выберете одного пользователя\");\n      } else {\n        this.isOpened = true;\n        this.choosenUser = this.items[this.checkedUsers[0]];\n      }\n    },\n    changeUser(newUsersData) {\n      this.$emit('parent_change_user', newUsersData);\n    },\n    openModalSingle(item) {\n      this.choosenUser = item;\n    }\n  }\n\n  /////////////////////////////////\n};","map":{"version":3,"names":["ProductModal","name","components","props","items","Array","data","modal1","modal2","checkedUsers","inputDisabled","isOpened","choosenUser","watch","length","methods","addElement","removeElement","openModalEdit","alert","changeUser","newUsersData","$emit","openModalSingle","item"],"sources":["C:\\Users\\aser\\PhpstormProjects\\exercise2\\src\\components\\products\\InfoProducts.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div>\r\n      <button type=\"button\" class=\"btn btn-success\" :data-bs-toggle=\"modal1\" data-bs-target=\"#addModal\"\r\n              @click=\"addElement\">Добавить</button>\r\n      <button type=\"button\" class=\"btn btn-warning\" :data-bs-toggle=\"modal2\" data-bs-target=\"#exampleModal\"\r\n              @click=\"openModalEdit\">Редактировать</button>\r\n      <button type=\"button\" class=\"btn btn-danger\" @click=\"removeElement\">Удалить</button>\r\n\r\n    </div>\r\n    <table class=\"table\">\r\n      <thead>\r\n      <tr>\r\n        <th scope=\"col\"></th>\r\n        <th scope=\"col\">Id</th>\r\n        <th scope=\"col\">Title</th>\r\n        <th scope=\"col\">Price</th>\r\n        <th scope=\"col\">Rating</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"(item, index) in items\" :key=\"item.id\">\r\n        <input class=\"form-check-input\" type=\"checkbox\" :value=\"index\" v-model=\"checkedUsers\"\r\n               :disabled=\"inputDisabled\" :id=\"item.id\">\r\n        <th scope=\"row\"><label class=\"form-check-label\" :for=\"item.id\">\r\n          {{ item.id }}\r\n        </label></th>\r\n        <td><label class=\"form-check-label\" :for=\"item.id\">\r\n          {{ item.title}}\r\n        </label></td>\r\n        <td><label class=\"form-check-label\" :for=\"item.id\">\r\n          {{ item.price}}\r\n        </label></td>\r\n        <td><label class=\"form-check-label\" :for=\"item.id\">\r\n          {{ item.rating}}\r\n        </label></td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n\r\n  <ProductModal :choosenUser=\"choosenUser\" @change_user=\"changeUser\"/>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nimport ProductModal from \"@/components/products/ProductModal\";\r\n\r\nexport default {\r\n  name: \"InfoProducts\",\r\n  components:{\r\n    ProductModal\r\n  },\r\n  props: {\r\n    items: Array,\r\n  },\r\n  data() {\r\n    return {\r\n      modal1: null,\r\n      modal2: null,\r\n      checkedUsers: [],\r\n      inputDisabled: false,\r\n      isOpened: false,\r\n      choosenUser: {}\r\n    }\r\n  },\r\n  watch: {\r\n    checkedUsers(){\r\n      if (this.checkedUsers.length > 1){\r\n        this.modal1 = null\r\n      }else{\r\n        if(this.checkedUsers.length === 1){\r\n          this.modal1 = \"modal\"\r\n        }else {\r\n          this.inputDisabled =false\r\n          this.modal1 = null\r\n        }\r\n        this.inputDisabled =false\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    addElement(){\r\n      this.modal2 = \"modal\"\r\n    },\r\n\r\n    removeElement() {\r\n      this.checkedUsers\r\n    },\r\n    openModalEdit() {\r\n      if (this.checkedUsers.length !== 1){\r\n        this.modal1 = null\r\n        alert(\"Выберете одного пользователя\")\r\n      }\r\n      else{\r\n        this.isOpened = true\r\n        this.choosenUser = this.items[this.checkedUsers[0]]\r\n      }\r\n    },\r\n    changeUser(newUsersData) {\r\n      this.$emit('parent_change_user', newUsersData)\r\n    },\r\n    openModalSingle(item){\r\n      this.choosenUser = item\r\n    }\r\n  }\r\n\r\n/////////////////////////////////\r\n\r\n\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n#users, #users_action {\r\n  margin-top: 2%;\r\n}\r\n\r\n#users_action button {\r\n  margin-left: 2%;\r\n}\r\n#users_card {\r\n  margin-top: 2%;\r\n}\r\n.cards {\r\n  margin-bottom: 2%;\r\n}\r\n\r\n.btn {\r\n  margin: 1%;\r\n}\r\n</style>"],"mappings":"AA+CA,OAAOA,YAAW,MAAO,oCAAoC;AAE7D,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAC;IACTF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,KAAK,EAAEC;EACT,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,MAAM,EAAE,IAAI;MACZC,YAAY,EAAE,EAAE;MAChBC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE,KAAK;MACfC,WAAW,EAAE,CAAC;IAChB;EACF,CAAC;EACDC,KAAK,EAAE;IACLJ,YAAYA,CAAA,EAAE;MACZ,IAAI,IAAI,CAACA,YAAY,CAACK,MAAK,GAAI,CAAC,EAAC;QAC/B,IAAI,CAACP,MAAK,GAAI,IAAG;MACnB,CAAC,MAAI;QACH,IAAG,IAAI,CAACE,YAAY,CAACK,MAAK,KAAM,CAAC,EAAC;UAChC,IAAI,CAACP,MAAK,GAAI,OAAM;QACtB,CAAC,MAAK;UACJ,IAAI,CAACG,aAAY,GAAG,KAAI;UACxB,IAAI,CAACH,MAAK,GAAI,IAAG;QACnB;QACA,IAAI,CAACG,aAAY,GAAG,KAAI;MAC1B;IACF;EACF,CAAC;EAEDK,OAAO,EAA