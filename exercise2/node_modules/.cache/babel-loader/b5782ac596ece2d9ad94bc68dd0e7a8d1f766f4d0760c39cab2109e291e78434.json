; // Первое вычисляемое свойство\r\n    },\r\n  },\r\n\r\n  async mounted() {\r\n    this.$store.dispatch('fetchClients')\r\n  }\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.btntd{\r\n  width: 150px;\r\n}\r\n</style>"],"mappings":"AA4BA,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,SAAQC,UAAU,QAAO,MAAM;AAG/B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAK,EAAE;IACLC,eAAeA,CAAA,EAAE;MACf,IAAI,IAAI,CAACA,eAAe,CAACC,MAAK,GAAI,CAAC,EAAC;QAClC,IAAI,CAACL,MAAK,GAAI,IAAG;MACnB,CAAC,MAAI;QACH,IAAG,IAAI,CAACI,eAAe,CAACC,MAAK,KAAM,CAAC,EAAC;UACnC,IAAI,CAACL,MAAK,GAAI,OAAM;QACtB,CAAC,MAAK;UACJ,IAAI,CAACC,aAAY,GAAG,KAAI;UACxB,IAAI,CAACD,MAAK,GAAI,IAAG;QACnB;QACA,IAAI,CAACC,aAAY,GAAG,KAAI;MAC1B;IACF;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,WAAW,CAAC,EAAE;IACrC,CAAC;;IACDC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACF,MAAM,CAACC,QAAQ,CAAC,YAAY,CAAC,EAAE;IACtC,CAAC;;IAGDE,UAAUA,CAAA,EAAE;MACV,IAAI,CAACX,MAAK,GAAI,OAAM;MACpB,IAAI,CAACE,QAAO,GAAI,IAAG;IACrB;EACF,CAAC;EACDU,QAAQ,EAAC;IACL,GAAGhB,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAC/BiB,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACL,MAAM,CAACM,KAAK,CAACC,KAAK,CAACF,WAAW,EAAE;IAC9C;EACF,CAAC;;EAED,MAAMG,OAAOA,CAAA,EAAG;IACd,IAAI,CAACR,MAAM,CAACC,QAAQ,CAAC,cAAc;EACrC;AAEF"},"metadata":{},"sourceType":"module","externalDependencies":[]}