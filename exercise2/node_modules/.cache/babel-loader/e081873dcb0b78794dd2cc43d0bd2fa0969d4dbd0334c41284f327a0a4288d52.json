{"ast":null,"code":"import AddModal from \"@/components/AddModal\";\nimport UpdateModal from \"@/components/UpdateModal\";\nimport axios from \"axios\";\nimport { mapGetters } from 'vuex';\nexport default {\n  name: \"ClientList\",\n  components: {\n    AddModal,\n    UpdateModal\n  },\n  data() {\n    return {\n      modal1: null,\n      modal2: null,\n      inputDisabled: false,\n      isOpened: false,\n      choosenClient: {},\n      deleteClient: {},\n      checkedClient: []\n    };\n  },\n  methods: {\n    changeUserData(newUsersData) {\n      let index = this.usersArray.findIndex(i => i.id === newUsersData['id']);\n      this.usersArray[index] = newUsersData;\n    },\n    addElement() {\n      this.modal1 = \"modal\";\n      this.isOpened = true;\n    },\n    addElement2(item) {\n      this.modal2 = \"modal\";\n      this.isOpened = true;\n      this.choosenClient = item;\n    },\n    changedelete(item) {\n      this.deleteClient = item;\n      axios.delete(`http://127.0.0.1:8000/api/clients/delete-clients/${this.deleteClient.id}`);\n    }\n  },\n  computed: {\n    ...mapGetters([\"allClients\"]),\n    isModalOpen() {\n      return this.$store.state.modal.isModalOpen; // Первое вычисляемое свойство\n    }\n  },\n\n  async mounted() {\n    this.$store.dispatch('fetchClients');\n  }\n};","map":{"version":3,"names":["AddModal","UpdateModal","axios","mapGetters","name","components","data","modal1","modal2","inputDisabled","isOpened","choosenClient","deleteClient","checkedClient","methods","changeUserData","newUsersData","index","usersArray","findIndex","i","id","addElement","addElement2","item","changedelete","delete","computed","isModalOpen","$store","state","modal","mounted","dispatch"],"sources":["C:\\Users\\aser\\PhpstormProjects\\exercise2\\src\\views\\ClientList.vue"],"sourcesContent":["<template>\r\n  <table class=\"table\">\r\n    <thead>\r\n    <tr>\r\n      <th scope=\"col\"></th>\r\n      <th scope=\"col\">Client Id</th>\r\n      <th scope=\"col\">Lastname</th>\r\n      <th scope=\"col\">Firstname</th>\r\n      <th scope=\"col\"><button type=\"button\" class=\"btn btn-success\" :data-bs-toggle=\"modal1\" data-bs-target=\"#addModal\"\r\n                                                      @click=\"addElement\">Добавить</button></th>\r\n      <th scope=\"col\"></th>\r\n    </tr>\r\n    </thead>\r\n    <tbody>\r\n    <tr v-for=\"item in allClients\" :key=\"item.id\">\r\n      <input class=\"form-check-input\" type=\"checkbox\" :value=\"index\" v-model=\"checkedProducts\"\r\n             :disabled=\"inputDisabled\" :id=\"item.id\">\r\n      <th scope=\"col\"></th>\r\n      <td>{{ item.id }}</td>\r\n      <td>{{ item.last_name_doc }}</td>\r\n      <td>{{ item.first_name_doc }}</td>\r\n      <td class=\"btntd\"><button type=\"button\" class=\"btn btn-warning\" :data-bs-toggle=\"modal2\" data-bs-target=\"#exampleModal\"\r\n                  @click=\"addElement2(item)\" >Редактировать</button></td>\r\n      <td class=\"btntd\"><button type=\"button\" class=\"btn btn-danger\" @click=\"changedelete(item)\">Удалить</button></td>\r\n      <!-- Вывод других данных -->\r\n    </tr>\r\n    </tbody>\r\n  </table>\r\n  <AddModal/>\r\n  <UpdateModal :choosenClient=\"choosenClient\"/>\r\n</template>\r\n\r\n<script>\r\nimport AddModal from \"@/components/AddModal\";\r\nimport UpdateModal from \"@/components/UpdateModal\";\r\nimport axios from \"axios\";\r\nimport {mapGetters} from 'vuex';\r\n\r\n\r\nexport default {\r\n  name: \"ClientList\",\r\n  components: {\r\n    AddModal,\r\n    UpdateModal\r\n  },\r\n  data() {\r\n    return {\r\n      modal1: null,\r\n      modal2: null,\r\n      inputDisabled: false,\r\n      isOpened: false,\r\n      choosenClient: {},\r\n      deleteClient: {},\r\n      checkedClient:[],\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    changeUserData(newUsersData) {\r\n      let index = this.usersArray.findIndex(i => i.id === newUsersData['id'])\r\n      this.usersArray[index] = newUsersData\r\n    },\r\n\r\n    addElement(){\r\n      this.modal1 = \"modal\"\r\n      this.isOpened = true\r\n    },\r\n    addElement2(item){\r\n      this.modal2 = \"modal\"\r\n      this.isOpened = true\r\n      this.choosenClient = item\r\n    },\r\n    changedelete(item){\r\n      this.deleteClient = item;\r\n      axios.delete(`http://127.0.0.1:8000/api/clients/delete-clients/${this.deleteClient.id}`);\r\n    }\r\n  },\r\n  computed:{\r\n      ...mapGetters([\"allClients\"]),\r\n    isModalOpen() {\r\n      return this.$store.state.modal.isModalOpen; // Первое вычисляемое свойство\r\n    },\r\n  },\r\n\r\n  async mounted() {\r\n    this.$store.dispatch('fetchClients')\r\n  }\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.btntd{\r\n  width: 150px;\r\n}\r\n</style>"],"mappings":"AAiCA,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAAQC,UAAU,QAAO,MAAM;AAG/B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVL,QAAQ;IACRC;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI;MACZC,MAAM,EAAE,IAAI;MACZC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE,KAAK;MACfC,aAAa,EAAE,CAAC,CAAC;MACjBC,YAAY,EAAE,CAAC,CAAC;MAChBC,aAAa,EAAC;IAChB;EACF,CAAC;EAEDC,OAAO,EAAE;IACPC,cAAcA,CAACC,YAAY,EAAE;MAC3B,IAAIC,KAAI,GAAI,IAAI,CAACC,UAAU,CAACC,SAAS,CAACC,CAAA,IAAKA,CAAC,CAACC,EAAC,KAAML,YAAY,CAAC,IAAI,CAAC;MACtE,IAAI,CAACE,UAAU,CAACD,KAAK,IAAID,YAAW;IACtC,CAAC;IAEDM,UAAUA,CAAA,EAAE;MACV,IAAI,CAACf,MAAK,GAAI,OAAM;MACpB,IAAI,CAACG,QAAO,GAAI,IAAG;IACrB,CAAC;IACDa,WAAWA,CAACC,IAAI,EAAC;MACf,IAAI,CAAChB,MAAK,GAAI,OAAM;MACpB,IAAI,CAACE,QAAO,GAAI,IAAG;MACnB,IAAI,CAACC,aAAY,GAAIa,IAAG;IAC1B,CAAC;IACDC,YAAYA,CAACD,IAAI,EAAC;MAChB,IAAI,CAACZ,YAAW,GAAIY,IAAI;MACxBtB,KAAK,CAACwB,MAAM,CAAE,oDAAmD,IAAI,CAACd,YAAY,CAACS,EAAG,EAAC,CAAC;IAC1F;EACF,CAAC;EACDM,QAAQ,EAAC;IACL,GAAGxB,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAC/ByB,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,KAAK,CAACH,WAAW,EAAE;IAC9C;EACF,CAAC;;EAED,MAAMI,OAAOA,CAAA,EAAG;IACd,IAAI,CAACH,MAAM,CAACI,QAAQ,CAAC,cAAc;EACrC;A